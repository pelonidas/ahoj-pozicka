    <link rel="stylesheet" href="https://unpkg.com/swiper@8/swiper-bundle.min.css"/>
    <link rel="stylesheet" href="/stylesheets/output.css">
    <link rel="stylesheet" href="/stylesheets/css/custom.css">
    <link rel="stylesheet" href="/stylesheets/css/rangeInput.css">

</head>
<body class="font-body text-secondary">
<div class="bg-secondary">
    <header class="w-full pt-6 md:pt-16 max-w-[92%] lg:max-w-[1620px] mx-auto">
        <%- include('../partials/navbar')%>
    </header>
</div>
<!--Content from other .ejs files-->
<%- body %>
<%- include('../partials/footer')%>
<!--<script src="/javascripts/calculator.js"></script>-->
</body>
<script>
    const body = document.querySelector('body');
    const html = document.querySelector('html');
    const nav = document.querySelector('nav')
    const content = document.querySelector('.nav-content');
    const container = content.parentElement
    const logo = document.querySelector('#logo');
    const hamburger = document.querySelector('.nav-hamburger')
    const pozicat = document.querySelector('#pozicat-button')

    hamburger.addEventListener('click', () => {
        content.classList.toggle('translate-x-full')
        body.classList.toggle('overflow-y-hidden')
        html.classList.toggle('overflow-y-hidden')
    })
    pozicat.addEventListener('click', () => {
        content.classList.toggle('translate-x-full')
        body.classList.remove('overflow-y-hidden')
        html.classList.remove('overflow-y-hidden')
    })

</script>
<script src="https://unpkg.com/swiper@8/swiper-bundle.min.js"></script>
<script src="/javascripts/swiper.js"></script>
<script src="/javascripts/calculator.js"></script>
<script src="/javascripts/autoReviews.js"></script>
<script src="/javascripts/validator.js"></script>
<script src="/javascripts/scripts.js"></script>
</html>
